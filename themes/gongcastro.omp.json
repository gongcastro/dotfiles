{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "newline": false,
        "segments": [
          {
            "foreground": "green",
            "properties": {
              "style": "folder"
            },
            "style": "powerline",
            "template": "\ue5ff {{ .Path }}",
            "type": "path"
          },
          {
            "foreground": "green",
            "foreground_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#07a9fa{{ end }}",
              "{{ if gt .Ahead 0 }}green{{ end }}",
              "{{ if gt .Behind 0 }}green{{ end }}"
            ],
            "properties": {
              "branch_max_length": 25,
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "plain",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          }
        ],
        "type": "prompt",
        "newline": true
      },
      {
        "alignment": "right",
        "segments": [
          {
            "type": "status",
            "style": "plain",
            "foreground": "green",
            "foreground_templates": [
              "{{ if gt .Code 0 }}green{{ end }}"
            ],
            "template": " x{{ reason .Code }}"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
  	    "type": "python",
  	    "style": "plain",
  	    "foreground": "#306998",
  	    "template": "Python {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}({{ .Venv }}){{ end }}{{ end }} "
	  },
	  {
  	    "type": "julia",
  	    "style": "plain",
  	    "foreground": "#9558B2",
  	    "template": " î˜¤ Julia "
          },
          {
  	    "type": "r",
  	    "style": "plain",
	    "newline": true,
  	    "foreground": "blue",
            "template": " R "
	  },
          {
            "foreground": "green",
            "style": "plain",
            "template": "\u0021",
            "type": "root"
          },
          {
            "style": "plain",
            "template": "\u276f",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
